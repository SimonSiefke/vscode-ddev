{
  "name": "ddev",
  "displayName": "ddev",
  "description": "ddev integration for vscode",
  "version": "0.0.0-semantically-released",
  "publisher": "SimonSiefke",
  "icon": "ddev_icon.png",
  "license": "MIT",
  "homepage": "https://github.com/SimonSiefke/vscode-ddev/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/SimonSiefke/vscode-ddev"
  },
  "bugs": "https://github.com/SimonSiefke/vscode-ddev/issues",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.ddevStart",
    "onCommand:extension.ddevStop",
    "onCommand:extension.ddevConfig",
    "onCommand:extension.ddevComposerInstall",
    "onCommand:extension.ddevImportDB",
    "onCommand:extension.ddevExportDB"
  ],
  "main": "./dist/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "ddev configuration",
      "properties": {
        "ddev.autoOpen": {
          "type": "boolean",
          "default": true,
          "description": "Automatically open the browser when ddev starts"
        },
        "ddev.defaultBrowser": {
          "type": "string",
          "default": "firefox",
          "description": "Use this browser"
        },
        "ddev.loglevel": {
          "type": "string",
          "default": "firefox",
          "description": "Use this browser",
          "oneOf": [
            "silent",
            "debug"
          ]
        }
      }
    },
    "commands": [
      {
        "command": "extension.ddevConfig",
        "title": "ddev: Config"
      },
      {
        "command": "extension.ddevStart",
        "title": "ddev: Start"
      },
      {
        "command": "extension.ddevStop",
        "title": "ddev: Stop"
      },
      {
        "command": "extension.ddevComposerInstall",
        "title": "ddev: Composer install"
      },
      {
        "command": "extension.ddevImportDB",
        "title": "ddev: Import db"
      },
      {
        "command": "extension.ddevExportDB",
        "title": "ddev: Export db"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "lint": "eslint . --ext .ts",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@types/glob": "^7.1.1",
    "@types/node": "^10.12.21",
    "@types/opn": "^5.1.0",
    "@types/url-parse": "^1.4.2",
    "eslint": "^5.13.0",
    "eslint-config-bitworkers": "^1.14.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "prettier-config-bitworkers": "^1.4.1",
    "semantic-release": "^15.13.3",
    "semantic-release-vsce": "^2.1.2",
    "ts-loader": "^5.3.3",
    "typescript": "3.3.1",
    "vscode": "^1.1.28",
    "webpack": "^4.29.1",
    "webpack-cli": "^3.2.3"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "verifyConditions": [
      "@semantic-release/changelog",
      "semantic-release-vsce",
      "@semantic-release/github"
    ],
    "prepare": [
      {
        "path": "@semantic-release/changelog"
      },
      {
        "path": "semantic-release-vsce",
        "packageVsix": "ddev.vsix"
      }
    ],
    "publish": [
      "semantic-release-vsce",
      {
        "path": "@semantic-release/github",
        "assets": "ddev.vsix"
      }
    ]
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "glob": "^7.1.3",
    "opn": "^5.4.0",
    "url-parse": "^1.4.4"
  }
}
