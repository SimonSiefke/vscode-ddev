{
  "name": "ddev",
  "displayName": "ddev",
  "description": "ddev integration for vscode",
  "version": "0.0.8",
  "publisher": "SimonSiefke",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.ddevStart",
    "onCommand:extension.ddevComposerInstall"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.ddevStart",
        "title": "ddev: Start"
      },
      {
        "command": "extension.ddevComposerInstall",
        "title": "ddev: Composer install"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "publish": "vsce publish",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@types/node": "^10.12.21",
    "eslint": "^5.13.0",
    "eslint-config-bitworkers": "^1.14.0",
    "prettier": "^1.16.4",
    "prettier-config-bitworkers": "^1.4.1",
    "semantic-release": "^15.13.3",
    "semantic-release-vsce": "^2.1.2",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "release": {
    "verifyConditions": [
      "semantic-release-vsce",
      "@semantic-release/github"
    ],
    "prepare": {
      "path": "semantic-release-vsce",
      "packageVsix": "ddev.vsix"
    },
    "publish": [
      "semantic-release-vsce",
      {
        "path": "@semantic-release/github",
        "assets": "ddev.vsix"
      }
    ]
  }
}
